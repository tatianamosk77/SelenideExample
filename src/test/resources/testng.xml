<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Demo QA Suite">
    <listeners>
        <listener class-name="listener.CustomListener"/>
        <listener class-name="listener.RetryListener"/>
    </listeners>
    <parameter name="projectId" value="1"/>
    <test name="DemoQA Pages">
        <parameter name="browser" value="Chrome"/>
        <parameter name="browserVersion" value="117"/>
        <packages>
            <package name="ui"/>
        </packages>
    </test>
</suite>